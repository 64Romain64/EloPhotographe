<!-- HEADER -->
<header
	class="right-menu ">
	<!-- LOGO -->
	<a href="{{path('main')}}" class="logo"><img src="{{ asset ('images/Logo.png') }}" alt=""></a>
	<!-- /LOGO -->
	<!-- MOB MENU ICON -->
	<a href="{{path('main')}}" class="mob-nav">
		<i class="fa fa-bars"></i>
	</a>
	<!-- /MOB MENU ICON -->
	<!-- ASIDE MENU ICON -->
	<a href="{{path('main')}}" class="aside-nav">
		<span class="aside-nav-line line-1"></span>
		<span class="aside-nav-line line-2"></span>
		<span class="aside-nav-line line-3"></span>
	</a>
	<!-- /ASIDE MOB MENU ICON -->
	<!-- NAVIGATION -->
	<nav id="topmenu" class="topmenu">
		<ul class="menu">
			<li class="menu-item menu-item-type-custom menu-item-object-custom menu-item-has-children">
				<a href="{{path('main')}}">HOME</a>
			</li>
			<li class="menu-item menu-item-type-custom menu-item-object-custom menu-item-has-children">
				<a href="#">PROFESSIONNEL</a>
				<ul class="sub-menu">
					{% for categorie in categorieNavBar() %}
						{% if categorie.titre == "Professionnel" %}
							<li class="menu-item menu-item-type-custom menu-item-object-custom">
								<a href="{{ path ( 'professionnel' , {slug: categorie.slug}) }}">Photos
									{{categorie.nom}}</a>
							</li>
							{# <li class="menu-item menu-item-type-custom menu-item-object-custom"><a href="#">Photos {{categorie.nom}}</a></li> #}
						{% endif %}
					{% endfor %}
				</ul>
			</li>
			<li class="menu-item menu-item-type-custom menu-item-object-custom menu-item-has-children">
				<a href="#">PHOTOGRAPHIE D'ART</a>
				<ul class="sub-menu">
					{% for categorie in categorieNavBar() %}
						{% if categorie.titre == "Art" %}
							<li class="menu-item menu-item-type-custom menu-item-object-custom">
								<a href="{{ path ( 'professionnel' , {slug: categorie.slug}) }}">Photos
									{{categorie.nom}}</a>
							</li>
						{% endif %}
					{% endfor %}
				</ul>
			</li>
			{# <li class="menu-item menu-item-type-custom menu-item-object-custom menu-item-has-children mega-menu">
																						<a href="{{path ('visites')}}">Visites Virtuelles</a>
																					</li> #}
			<li class="menu-item menu-item-type-custom menu-item-object-custom menu-item-has-children">
				<a href="{{path ('about')}}">ABOUT</a>
			</li>
			<li class="menu-item menu-item-type-custom menu-item-object-custom menu-item-has-children mega-menu">
				<a href="{{path ('contact')}}">CONTACT</a>
			</li>
		</ul>
		<div class="f-right">
			<div class="whizz-top-social">
				<span class="social-icon fa fa-share-alt"></span>

				<ul class="social">
					{% if photographe is defined %}
						<li>
							<a href="{{photographe.facebook}}" target="_blank">
								<i class="fa fa-facebook"></i>
							</a>
						</li>
						<li>
							<a href="{{photographe.instagram}}" target="_blank">
								<i class="fa fa-instagram"></i>
							</a>
						</li>
						<li>
							<a href="{{photographe.linkedin}}" target="_blank">
								<i class="fa fa-linkedin"></i>
							</a>
						</li>
					{% endif %}

					{% if is_granted('ROLE_ADMIN') %}
						<li>
							<a style="color:blue" href="{{path ('160612admin230211')}}" target="_blank">
								<i class="fa fa-user"></i>
								ADMIN
							</a>
							{# <p>{{ app.user.nom }}</p> #}
						</li>
						<li>
							<a style="color:red" href="{{ path('app_logout') }}">
								<i class="fa fa-sign-out"></i>Deconnexion
							</a>
						</li>
					{% endif %}
				</ul>
			</div>
		</div>
	</nav>
	<!-- NAVIGATION -->
	<a href="#" class="socials-mob-but">
		<i class="fa fa-share-alt"></i>
	</a>
</header>
