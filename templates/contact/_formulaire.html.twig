<div class="my-custom-class-for-errors">{{form_errors(form)}}</div>

<div class="vc_column_container vc_col-sm-6 vc_col-lg-7 ">
	<div class="vc_column-inner ">
		<div>
			<div role="form" class="wpcf7" id="wpcf7-f2623-p4036-o1" lang="fr-FR" dir="ltr">
				<div class="screen-reader-response"></div>
				<form method="post" class="wpcf7-form" novalidate="novalidate">

					{{ form_start(form)}}
					{% for message in app.flashes('success')%}
						<div class="alert alert-success">{{message}}</div>
					{% endfor %}
					<div class="container">
						<div class="row">
							<div class="col-xs-12 col-md-6">
								<label>{{form_label(form.nom)}}<br/></label>
								{{form_widget(form.nom)}}
							</div>
							<div class="col-xs-12 col-md-6">
								<label>{{form_label(form.email)}}<br/></label>
								{{form_widget(form.email)}}
							</div>
						</div>
						<div class="row">
							<div class="col-xs-12">
								<label>{{form_label(form.message)}}<br/></label>
									{{form_widget(form.message)}}
							</div>
						</div>
						<div class="row">
							{{form_widget(form.envoyer)}}
						</div>
					</div>
					<div class="wpcf7-response-output wpcf7-display-none"></div>
					{{form_end(form)}}
				</form>
			</div>
		</div>
	</div>
</div>
<div class="kswr-column-settings" data-minheight="0px" data-isminheight="off" data-theclasses="kswr_pnone kswr_mnone kswr_bnone"></div>
